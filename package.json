{
  "name": "pattypm",
  "description": "A JavaScript value checker for NodeJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Linkurious/pattypm.git"
  },
  "author": {
    "name": "David Rapin",
    "url": "https://github.com/davidrapin"
  },
  "contributors": [
    {
      "name": "Francesco Infante",
      "url": "https://github.com/francescoinfante"
    }
  ],
  "keywords": [
    "process",
    "manager",
    "pm",
    "patty"
  ],
  "main": "./src/Patty.js",
  "license": "MIT",
  "version": "1.0.12",
  "private": false,
  "engines": {
    "node": "v7.5.0"
  },
  "bin": {
    "pattypm": "./bin/client.js",
    "pattypm-server": "./bin/server.js"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "//": "invoked using >npm run [script-name]",
    "test": "mocha ./test",
    "cover": "istanbul cover --root ./ -i '**/*.js' _mocha",
    "doc": "jsdoc --configure .jsdoc.json",
    "lint": "eslint ./src",
    "patch": "npm version patch && git push && npm publish"
  },
  "dependencies": {
    "bluebird": "3.4.7",
    "fs-extra": "2.0.0",
    "valcheck": "^1.0.8",
    "blessed": "0.1.81",
    "tree-kill": "1.1.0",
    "log4js": "1.1.1",
    "humanize": "0.0.9"
  },
  "devDependencies": {
    "mocha": "3.0.2",
    "should": "11.1.0",
    "eslint": "3.9.1",
    "eslint-plugin-import-order": "^2.1.4",
    "ink-docstrap": "1.1.0",
    "istanbul": "~1.1.0-alpha.1",
    "jsdoc": "3.4.1"
  }
}
